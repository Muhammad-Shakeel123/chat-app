{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/auth(.*)",
      "src": "/api/v1/user(.*)",
      "src": "/api/v1/session(.*)",
      "src": "/api/v1/message(.*)",
      "dest": "/src/index.js"
    }
  ]

}
