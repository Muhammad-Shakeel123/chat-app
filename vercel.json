{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/docs/(.*)",
      "dest": "/public/swagger-ui/$1"
    },
    {
      "src": "/swagger-ui/(.*)",
      "dest": "/public/swagger-ui/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "static": {
    "public": "public"
  }
}
